@sample



rate = srate/20;
//rate = 1;

devmode ? (
  (mtrc += 1) < rate ? (
    mtrinsrc = max(mtrinsrc, max(abs(spl0),abs(spl1)) );
  ):(
    mtrin = mtrinsrc;
    mtrinsrc = 0;
  );
);

spl0 == 0 && spl1 == 0 && mouse_cap == 0 ? (
  sleep == 0 ? sleepc += 1;
  sleepc > srate*2 ? sleep = 1;
):(
  sleep = sleepc = 0;
);

sleep == 0 ? (
  
  spinL = spl0;
  spinR = spl1;
  
  opL = spl0;
  opR = spl1;
  
  bypass == 0 || bypass == 2 ? (
    sp0tmp = spl0;
    sp1tmp = spl1;
    
    eq1.process(1);
    eq2.process(2);
    eq3.process(3);
    eq4.process(4);
    eq5.process(5);
    eq6.process(6);
    eq7.process(7);
    eq8.process(8);
    eq9.process(9);
    eq10.process(10);
  
    
    mutespls > 0 ? (
      mutespls -= 1;
      mute > 0 ? mute -= 1;
      mute < 0 ? mute = 0;
    ):(
      mute < 1 ? mute += 0.001;
      mute > 1 ? mute = 1;
    );
    
    soloid > 0 ? (
      spl0 = soloL;
      spl1 = soloR;
    );
  
    volout = 2^( (slider42-autovoldb) /6);
    spl0 *= volout *mute;
    spl1 *= volout *mute;
  );
    
    
  
  sp12 = max(abs(spl0),abs(spl1));
  sp12 > 2 ? (
    slider42 /= (sp12/2);
    slider_automate(slider42);
    
    redvol = 1;
  );
  
  
  //spl0 > 2 ? spl0 = 2;
  //spl1 > 2 ? spl1 = 2;
  soloid > 0 ? modeana = bufm[soloid];
  
  modeana < 1 ? (
    outl = spl0;
    outr = spl1;
  ) :
  modeana < 2 ? ( 
    outl = spl0 - (spl0-spl1);
    outr = spl1 - (spl1-spl0);
  //  outl = outr = spl0+spl1 - (spl0-spl1);
  ) :
  modeana < 3 ? (   
    outl = (spl0 - spl1) ;
    outr = 0;
  //  outr = (spl0 - spl1) ;
  ) :
  modeana < 4 ? (   
    outl = outr = spl0;
//    outl = outr = spl0 * (spl0-spl1)*2;
  ) :
  modeana < 5 ? (   
    outl = outr = spl1;
//    outl = outr = spl1 * (spl1-spl0)*2;
  );
    
  //outl *= roofgain;
  //outr *= roofgain;
  
  outl > 2 ? outl = 2;
  outl < -2 ? outl = -2;
  outr > 2 ? outr = 2;
  outr < -2 ? outr = -2;
  
  
  noanal == 0 ? (
  
    abs(recpos[]=(outl+outr)) > minvol ? update=1;
    //recpos[]=outl+outr;
    recpos = ((recpos+1) >= histsize ? 0 : (recpos+1));
  );
  
  
  

  // OSCILLOSCOPE
  //gmem[101] = ypx *100;
  //gmem[102] = zerog *100;
  //gmem[102] = ygr *100;
  //gmem[103] = yy *100;
  //gmem[104] = f *100;
  //gmem[103] = yy *100;
  //gmem[104] = y2y *100;
  
  

  
  
  
  fpsc += 1;
);

points == 0 ? (

  outl > pkL_ ? pkL_ = outl;
  outr > pkR_ ? pkR_ = outr;

  rmsL = rmsL.rms(outl);
  rmsR = rmsR.rms(outr);
);


devmode ? (
  (mtrc ) < rate ? (
    mtroutsrc = max(mtroutsrc, max(abs(spl0),abs(spl1)) );
  ):(
    mtrout = mtroutsrc;
    mtroutsrc = 0;
    mtrc = 0;
  );
);





