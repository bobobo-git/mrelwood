    ):(
      xy(xx = gfx_w - 3 - rmsw*.2 , yy);
      gfx_set(gr,.9,1, rmsa);
    );
      
    rmsL > -100 ? (
      gfx_drawstr( sprintf(#rmsL,"%.1f",rmsL) , 258,gfx_x,gfx_y);
    );
  ):(
    rmsR > -100 || rmsL > -100 ? (
      xy(xx = gfx_w - 3 - rmsw*.2 , yy);
      gfx_set(gr+=.1,gr,gr, rmsa);
