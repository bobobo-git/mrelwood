  floordb_  = (slider52/10)^2;
  slider60 = liftdb = -10.5;
  db2px = slider60/ ((slider60/gfx_h) * floordb_) ;
  liftdbpx = slider60 * db2px;
  liftdb = liftdbpx / db2px;
