    gfx_drawstr("Alt-click to ");
    mx1 = gfx_x;
    gfx_set(1,1,1,helpa);
    gfx_roundrect(gfx_x,gfx_y+ret,66*ret,gfx_texth , 6*ret , 1);
    gfx_x += 7*ret;
    slider41 <= 0 ? gfx_drawstr("Enable") : gfx_drawstr("Disable");
    gfx_set(.2,.7,.2,helpa);
