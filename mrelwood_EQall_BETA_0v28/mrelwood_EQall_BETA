  pkthr = .00001;
  pkdecay = .01;
  pkacc = .05 *0;
//  pkhold = srate/1000 *    10  ; // ms
  pkhold = 20  ; // fps

  
  pkL_ > pkL ? (
    pkL = pkL_;
    pkLh = 0;
  ):(
    (pkLh += 1) > pkhold ? pkL -= pkdecay;
  );
  pkLdb = 19.93 * log10(pkL);
  pkL_ = outl;

  pkR_ > pkR ? (
    pkR = pkR_;
    pkRh = 0;
  ):(
    (pkRh += 1) > pkhold ? pkR -= pkdecay;
  );
  pkRdb = 19.93 * log10(pkR);
  pkR_ = outr;

