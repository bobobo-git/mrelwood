  _codebufshape6 = bufshape[6];
  _codebufm1 = bufm[1];
  _codebufm2 = bufm[2];
  _codebufm3 = bufm[3];
  _codebufm4 = bufm[4];
  _codebufm5 = bufm[5];
  _codebufm6 = bufm[6];
  _codesol1 = bufdynsol[1];
  _codesol2 = bufdynsol[2];
  _codesol3 = bufdynsol[3];
  _codesol4 = bufdynsol[4];
