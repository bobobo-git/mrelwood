    gfx_set(1,gr,gr, pka);
    xy(xx = gfx_w - 3 , yy);
    gfx_drawstr( sprintf(#pkR,"%.1f",pkRdb) , 258,gfx_x,gfx_y);
  );
  pkL > pkthr ? (
    xy(xx - rmsw*.6 , yy);
    gfx_set(gr-.3,.9,1, pka);
