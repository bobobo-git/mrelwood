  blinkamax = (.9 - (helpa * .2));
  blinkamin = .1;
  blinkstop == 0 ? (blink2 = 1);
  blinkspd = 0.06 / (help/2+1);
  blinkdir == 0 ? (
    blink < 1 ? (
      blink += blinkspd;
    ):(
      blinkdir = 1;
    );
  ):(
    blink > 0 ? (

      blinkstop == 1 ? (
        blink -= blinkspd/4;
        blink2 = blink;
