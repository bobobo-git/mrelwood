  gfx_drawstr("RMS:",258,gfx_x,gfx_y);
  
  abs(rmsL-rmsR) > 3 ? (
    rmsR > -100 ? (
      gfx_set(1,gr,gr, rmsa);
      xy(xx = gfx_w - 3 , yy);
      gfx_drawstr( sprintf(#rmsR,"%.1f",rmsR) , 258,gfx_x,gfx_y);
      xy(xx - rmsw*.6 , yy);
