    this..poles > 2 ? out = out * (1-this..blend3) + this..blend3 *  this.p3.eqpole(out) ;
    this..poles > 3 ? out = out * (1-this..blend4) + this..blend4 *  this.p4.eqpole(out) ;
    this..poles > 4 ? out = out * (1-this..blend5) + this..blend5 *  this.p5.eqpole(out) ;
  );
  
  
  
  out;

);
