    my1 = gfx_y;
    my2 = gfx_y + gfx_texth;

    mouse_cap > mouse_capold && mouse_capold == 0 && mouse_x > mx1 && mouse_x < mx2
     && mouse_y > my1 && mouse_y < my2 ? (
      (slider50 += 1) > 1 ? slider50 = 0;
      noanal = 1-slider50;
