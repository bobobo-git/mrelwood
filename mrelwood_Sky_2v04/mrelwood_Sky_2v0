  starspeed -= (starx/fxw)^20 * 0.9 * starspeed + (1-starx/fxw)^20 * 0.9 * starspeed;
    stardir == 0 ? (
    starx >= fxw*2-4 ? stardir = -1;
    starx += starspeed;
    ):(
    starx <= 4 ? stardir = 0;
    starx -= starspeed;
    );
  );


