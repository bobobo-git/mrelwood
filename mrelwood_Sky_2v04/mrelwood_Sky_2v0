drawzero == 1 ? (
  drawzero = 0;

  // Binoculars 2
  gfx_dest = 14;
  gfx_setimgdim(14,-1,-1); gfx_setimgdim(14,80,80);
  gfx_set(1,1,1,1,0);
  xstart = 20;
  xstart2 = xstart + sz*1.25;
  ystart = 10;
  sz = 20;
  
  // Connectors
  gfx_set(1,1,1,1);
  gfx_rect(xstart+sz,ystart+sz*0.3,sz/2,4);
  gfx_rect(xstart+sz,ystart+sz*0.9,sz/2,6);
  // LEFT SIDE
  // Back wall
