zeromy2 = 80;
(mouse_x > (zeromx1)*ret1) && (mouse_x < (zeromx2)*ret1) && (mouse_y > (zeromy1)*ret1) && (mouse_y < (zeromy2)*ret1) ? (
  mouse_cap != 0 && cap_mode == 0 ? (
    cap_mode = lastcap = 31;
    );
  );


//
