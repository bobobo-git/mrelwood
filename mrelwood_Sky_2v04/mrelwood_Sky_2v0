gfx_rect(0,fxh-spad,fxw,spad);




cap_mode == 2 ? (
  abs(mouse_x - mxs) > r ? (helpout = 1);
  ):(

// Enable help for buttons:
  (cap_mode != 31) && (cap_mode != 32) && (cap_mode > 0) ? (
    abs(mouse_y - mys) > r ? (helpout = 1);
    );
