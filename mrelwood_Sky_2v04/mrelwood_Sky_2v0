upstars > 0 ? (
  gfx_dest = 3;
  gfx_setimgdim(3, -1, -1);   gfx_setimgdim(3, fxw*4, fxh*1); 
  loop( 20,
    gfx_x = rand(fxw*4);
    gfx_y = ( rand(1)^1.5 * fxh*1 );
    randradi = rand(2) +0.5;
    gfx_set(1,1,1, 1- rand(1)*0.3 -randradi*0.15 ,0);
    gfx_circle(gfx_x,gfx_y,randradi,1,1 );
    );
