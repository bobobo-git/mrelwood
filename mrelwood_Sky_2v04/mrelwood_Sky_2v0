// Title
(mouse_x > (titx)*ret1) && (mouse_x < (titx+titw*1.2)*ret1) && (mouse_y > (tity*2)*ret1) && (mouse_y < (tity*2+tith*1.2)*ret1) ? (
  mouse_cap != 0 && cap_mode == 0 ? (
    cap_mode = lastcap = 99;
    );
