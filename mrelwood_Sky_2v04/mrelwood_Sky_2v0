gfx_setfont(7);
gfx_rect(fxw/2-helpboxw/2,gfx_texth * 0.2, helpboxw,gfx_texth*1.2);
help > 0 ? (
  gfx_set(0.85,0.85,0.75,helpboxa);
  gfx_x = fxw/2;
