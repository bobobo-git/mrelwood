    drawceil = 1;
    );
  
  lastcap == 31 ? (
    slider31 == 0 ? (slider31 = 1) : (slider31 = 0);
    sliderchange(slider31);
    slider_automate(slider31);
    zerolatency = slider31;
    upslider = 1;
    dblclk = 0;
    );  
  
  lastcap == 32 ? (
